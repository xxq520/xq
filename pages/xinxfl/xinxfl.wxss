}
.cp-bom{border-bottom: #eee solid 0.5rpx;}
.search {
  background-color: #ffffff85;
  margin: 20rpx 40rpx;
  border-radius: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  flex-wrap: nowrap;
  padding: 0 30rpx;
  vertical-align: middle;
  align-items: center;
}

.search icon {
  margin-right: 10rpx;
  height: 18px;
}

.search input {
  flex: 1;
  vertical-align: middle;
  height: 70rpx;
  display: inline-block;
  font-size: 26rpx;
}
.scroller{
  -webkit-overflow-scrolling:touch;
}
.items.actives {
  /* border-bottom: rgb(255, 88, 94) solid 1rpx; */
