/* 返回顶部 */

.goTop {
  z-index: 9999;
  height: 88rpx;
  width: 88rpx;
  position: fixed;
  bottom: 20rpx;
  background-color: rgba(255, 255, 255, 0.829);
  right: 30rpx;
  border-radius: 50%;
  border: 1rpx solid #969696;
}

