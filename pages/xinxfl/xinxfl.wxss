@import '../../components/good/good.wxss';

page {
  background-color: #f5f5f5;
  overflow-y:auto !important;
}
.navbtns{background-color: #fff;padding-top: 10rpx;padding-bottom: 10rpx;}
.items {
  display: block;
  /* float: left; */
  /* width: 25%; */
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}
.ac {
  border-right: #585858 solid 2rpx;
