/* modal */

.modal {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 9999;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.modal .modal-content {
  width: 80%;
  background: #fff;
  border-radius: 16rpx;
}

.modal .modal-foot {
  /* border-top: 1px solid #f2f2f2; */
  display: flex;
  flex-direction: row;
}

.modal .modal-foot .confirm, .modal .modal-foot .cancel {
  width: 100%;
  padding: 30rpx;
  text-align: center;
}

